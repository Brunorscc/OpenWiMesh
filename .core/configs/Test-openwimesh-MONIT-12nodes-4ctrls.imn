node n1 {
    type router
    model router
    network-config {
	hostname n1
	!
	interface eth0
	 ip address 192.168.199.1/24
	!
    }
    canvas c1
    iconcoords {1016.0 768.0}
    labelcoords {1022.6666666666665 810.0}
    services {openwimesh SSH collector}
    custom-config {
	custom-config-id service:collector
	custom-command collector
	config {
	files=('collector.sh', )
	}
    }
    custom-config {
	custom-config-id service:collector:collector.sh
	custom-command collector.sh
	config {
	#!/bin/sh
	echo "$(date) - Starting Collector - IFSTAT"
	ifstat -n -t -i eth0 -b 5 > collector-ifstat.log &
	
	}
    }
    custom-config {
	custom-config-id service:openwimesh
	custom-command openwimesh
	config {
	dirs=('/var/lib/openvswitch', '/var/log', '/etc/openvswitch', )
	files=('openwimesh.conf', )
	}
    }
    custom-config {
	custom-config-id service:openwimesh:openwimesh.conf
	custom-command openwimesh.conf
	config {
	# auto-generated by OpenWiMesh (openwimesh.py)
	OWM_IFACES="eth0"
	
	OWM_OFCTL="192.168.199.254"
	
	
	}
    }
    interface-peer {eth0 n4}
}

